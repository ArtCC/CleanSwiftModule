<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.iphoneos</string>
	</array>
	<key>DefaultCompletionName</key>
	<string>CleanSwiftModule</string>
	<key>Description</key>
	<string>Clean Swift template: create complete scene with Models, View, ViewController, Presenter, Interactor, Router. Create Repository classes. Create Unit Test classes. Create view class.</string>
	<key>Kind</key>
	<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.swift</string>
	<key>SortOrder</key>
	<integer>90</integer>
	<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>Name of the module to create</string>
			<key>Identifier</key>
			<string>cleanSwiftModuleName</string>
			<key>Name</key>
			<string>Module name</string>
			<key>Default</key>
			<string>ModuleName</string>
			<key>Type</key>
			<string>text</string>
			<key>Placeholder</key>
			<string>Input Module Name</string>
			<key>Required</key>
			<true/>
			<key>NotPersisted</key>
			<true/>
		</dict>
		<dict>
			<key>Description</key>
			<string>Clean Swift module type</string>
			<key>Identifier</key>
			<string>moduleType</string>
			<key>Name</key>
			<string>Module Type</string>
			<key>Type</key>
			<string>popup</string>
			<key>Values</key>
			<array>
				<string>Scene</string>
				<string>Repository</string>
				<string>Unit Test</string>
				<string>View</string>
			</array>
			<key>Default</key>
			<string>Scene</string>
			<key>Required</key>
			<true/>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_cleanSwiftModuleName___</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Type</key>
			<string>static</string>
		</dict>
	</array>
	<key>Template Author</key>
	<string>ArtCC</string>
	<key>Summary</key>
	<string>This generates a new scene, repository, unit test or view using Clean Swift architecture for iOS.</string>
</dict>
</plist>
